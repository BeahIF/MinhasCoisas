package obi;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		//entrada: 43 23 87 23 12 \n
		// saida: 87\n
		
		Scanner scan = new Scanner(System.in);
		int max = 0;
		scan.useDelimiter(" ");
		while(scan.hasNextLine()){
			int num =  scan.nextInt();
			if(num > max){
				max = num;
			}
		}
			System.out.println(max);
		}
	}



